<%- model_class = Entry -%>
<div class="page-header">
  <h1><%=t '.title', :default => Entry %></h1>
</div>

Score: <%= @entry.score %>

  <video id="my-video" class="video-js" controls preload="auto" width="640" height="264"
  poster="MY_VIDEO_POSTER.jpg" data-setup="{}">
    <source src= '<%= @entry.video.video_url %>' type='video/mp4'>
    <!-- <source src="MY_VIDEO.webm" type='video/webm'> -->
    <p class="vjs-no-js">
      To view this video please enable JavaScript, and consider upgrading to a web browser that
      <a href="http://videojs.com/html5-video-support/" target="_blank">supports HTML5 video</a>
    </p>
  </video>
  <br />

<div class="form-group">
  <%= link_to 'Back to Profile',
              user_fact_path(@user), :class => 'btn btn-default'  %>
  <%= link_to 'Back to Event',
              event_path(@event), :class => 'btn btn-default'  %>
  <%= link_to 'Edit',
              edit_event_entry_path(@event, @entry), :class => 'btn btn-default' %>
  <%= link_to 'Destroy',
              event_entry_path(@event, @entry),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>

