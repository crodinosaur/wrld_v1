<%- model_class = Entry -%>
<div class="page-header">
  <h1><%=t '.title', :default => Entry %></h1>
</div>

Score: <%= @entry.score %>

<div class="fieldset">
  <dl>
  </dl>
</div>

<div class="form-group">
  <%= link_to 'Back to Profile',
              user_fact_path(@user), :class => 'btn btn-default'  %>
  <%= link_to 'Back to Event',
              event_path(@event), :class => 'btn btn-default'  %>
  <%= link_to 'Edit',
              edit_event_entry_path(@event, @entry), :class => 'btn btn-default' %>
  <%= link_to 'Destroy',
              event_entry_path(@event, @entry),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>

